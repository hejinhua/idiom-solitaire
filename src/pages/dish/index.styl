.wrapper {
  width: 100vw;
  background-color: #F5F5F5;
}
.bg {
  width: 100%;
  height: 270px;
  background: url('../../assets/images/search-bg.png') no-repeat;
  background-size: 100% 100%;
}
.title {
  display: flex;
  align-items: center;
  overflow: visible;
}
.logo {
  width: 62px;
  height: 90px;
  margin-left: 15px;  
}
.logo-text {
  color: #fff;
  font-size: 40px;
  margin-left: 32px;  
  line-height: 114px;
}
.content {
  width: 100%;
  position: absolute;
  left: 0;  
}
.series-title {
  font-size: 24px;
  line-height: 1;  
  padding-top: 32px;
  margin-bottom: 22px;
  display: flex;
  align-items: center;
  font-weight: bold;
}
.dashed-line {
  flex-grow: 1;
  height: 0;
  border-bottom: 1px dashed #E3E3E3;
  margin-left: 22px;  
}
