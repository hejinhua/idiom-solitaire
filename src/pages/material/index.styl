.wrapper {
  width: 100vw;
}
.bg {
  width: 100%;
  height: 270px;
  background: url('../../assets/images/search-bg.png') no-repeat;
  background-size: 100% 100%;
  position: absolute;
  z-index: -1;
  left: 0;
  top: 0;
}
.title {
  color: #fff;
  text-align: center;
  font-size: 40px;
  font-weight: bold;
}
.contentm {
  width: 100%;
  position: absolute;
  left: 0;  
}
.series-title {
  font-size: 24px;
  line-height: 1;  
  padding-top: 32px;
  margin-bottom: 22px;
  display: flex;
  align-items: center;
  font-weight: bold;
}
.dashed-line {
  flex-grow: 1;
  height: 0;
  border-bottom: 1px dashed #E3E3E3;
  margin-left: 22px;  
}